{
    "name": "tableau-wdc-dynamic",
    "displayName": "Dynamic Report Connector",
    "version": "1.0.0",
    "tableau-version": {
        "min": "2023.3"
    },
    "vendor": {
        "name": "Dynamic Reports Inc",
        "support-link": "https://github.com/your-org/tableau-wdc-dynamic",
        "email": "support@yourorg.com"
    },
    "permission": {
        "api": {
            "http://localhost:4173/*": [
                "GET",
